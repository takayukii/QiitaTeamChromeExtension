{
  "manifest_version": 2,
  "name": "Qiita Team Extended",
  "version": "1.1",
  "description": "",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://*.qiita.com/",
    "https://mandrillapp.com/"
  ],
  "icons" : {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
